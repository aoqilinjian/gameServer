// -----------------------------------------
// 测试消息
// -----------------------------------------
//从客户端发送到服务器
message C_TEST {
	option (msgid) = 9901;
	required int32 cscode = 1;		//CS测试编码
	optional string csstring = 2;	//CS测试字符串
}

//从服务器发送到客户端
message S_TEST {
	option (msgid) = 9902;
	required int32 sccode = 1;		//SC测试编码
	optional string scstring = 2;	//SC测试字符串
}